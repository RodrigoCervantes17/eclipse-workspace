package imsickofthisshit;

public class ifforfirsttime {
	public static void main(String[] args) {
		System.out.println("Hello World");
		int edad = 17;
		int cantidad = 3;
		if (edad >= 18) {
			System.out.println("Si, usted es mayor a 18.");
			System.out.println("Bienvenido.");
		}
		else {
			if (cantidad>=2) {
				System.out.println("Usted es menor de edad, pero es permitido su ingreso.");
			}
			System.out.println("No es bienvenido.");
		}

	}

}
